{
  "name": "Phanes",
  "env": "dev",
  "version": "0.1.0",
  "server": {
    "http": {
      "http_listen": ":7771",
      "validate_trans": "zh"
    },
    "grpc": {
      "grpc_listen": ":7090",
      "discovery_listen": "",
      "validate_trans": "zh"
    }
  },
  "cache": {
    "enabled": false,
    "type": "redis",
    "addr": "127.0.0.1:6379",
    "pwd": ""
  },
  "store": {
    "enabled": true,
    "auto_migrate": true,
    "db": [
      {
        "addr": "host=127.0.0.1 user=postgres password=root dbname=hello port=5432 sslmode=disable TimeZone=Asia/Shanghai",
        "pwd": "root",
        "type": "postgres",
        "user": "root",
        "enabled": false
      },
      {
        "addr": "127.0.0.1:6379",
        "pwd": "",
        "type": "redis",
        "user": "",
        "enabled": false
      }
    ]
  },
  "event": {
    "enabled": false,
    "topic_prefix": "phanes",
    "broker": {
      "addr": "",
      "pwd": "",
      "type": "kafka"
    }
  },
  "client": {
    "broker": {
      "enabled": false,
      "addr": "amqp://coco:kk123123123@127.0.0.1:5672/",
      "pwd": "",
      "type": "rabbitmq",
      "user": ""
    }
  },
  "collect": {
    "log": {
      "level": -1,
      "file_name": "admin",
      "prefix": "[PHANES]",
      "buffer_size": 4096,
      "interval": 10
    },
    "metric": {
      "listen": ":2223",
      "enabled": false
    },
    "trace": {
      "addr": "http://localhost:14268/api/traces",
      "enabled": true,
      "protocol": "http",
      "type": "jaeger"
    }
  },
  "traefik": {
    "type": "tcp",
    "rule": "&&",
    "tls": true,
    "domain": "test.com",
    "enabled": true
  }
}