{
  "name": "phanes",
  "env": "dev",
  "version": "0.1.0",
  "http": {
    "http_listen": ":7771",
    "validate_trans": "zh"
  },

  "grpc": {
    "grpc_listen": ":9090",
    "discovery_listen": "",
    "validate_trans": "zh"
  },

  "collect": {
    "log": {
      "prefix": "【Phanes】",
      "file_name": "admin",
      "redis": {
        "redis_key": "log_list",
        "addr": "",
        "pwd": ""
      }
    },
    "trace": {
      "enabled": false,
      "addr": ""
    },
    "metric": {
      "enabled": false,
      "addr": ""
    }
  },

  "db": [
    {
      "enabled": false,
      "type": "postgres",
      "addr": "host=127.0.0.1 user=root password=root dbname=phanes port=5432 sslmode=disable TimeZone=Asia/Shanghai",
      "user": "root",
      "pwd": "root"
    },
    {
        "enabled": false,
        "type": "redis",
        "addr": "127.0.0.1:6379",
        "user": "",
        "pwd": ""
    }
  ],

  "proxy": {
    "domain": "http://127.0.0.1",
    "prefix": "/phanes"
  },

  "broker": {
    "enabled": false,
    "type": "rabbitmq",
    "addr": "amqp://coco:kk123123123@127.0.0.1:5672/",
    "user": "",
    "pwd": ""
  },

  "traefik": {
    "enabled": false,
    "domain": "test.com",
    "prefix": "/phanes"
  }
}